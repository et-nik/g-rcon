{
    "name": "knik/g-rcon",
    "type": "library",
    "description": "RCON Library for game servers management",
    "keywords": [
        "rcon", "counter-strike", "half-life", "teamspeak"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Kuznetsov",
            "email": "nikita.hldm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Knik\\GRcon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Knik\\GRcon\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-openssl": "*",
        "ext-sockets": "*",
        "phpseclib/phpseclib": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3"
    }
}
